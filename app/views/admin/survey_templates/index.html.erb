<p id="notice"><%= notice %></p>

<h1>Admin Survey Templates</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @surveys.each do |survey| %>
      <tr>
        <td><%= link_to survey.title, edit_admin_survey_template_path(survey) %></td>
        <td><%= link_to 'Destroy', admin_survey_template_path(survey), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Survey Template', new_admin_survey_template_path %>
